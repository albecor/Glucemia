<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterPacient</servlet-name>
        <servlet-class>Controler.RegisterPacient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controler.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterPersonal</servlet-name>
        <servlet-class>Controler.RegisterPersonal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterGlucose</servlet-name>
        <servlet-class>Controler.RegisterGlucose</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListGlucose</servlet-name>
        <servlet-class>Controler.ListGlucose</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterPacient</servlet-name>
        <url-pattern>/RegisterPacient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterPersonal</servlet-name>
        <url-pattern>/RegisterPersonal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterGlucose</servlet-name>
        <url-pattern>/RegisterGlucose</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListGlucose</servlet-name>
        <url-pattern>/ListGlucose</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
